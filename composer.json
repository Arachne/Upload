{
    "name": "arachne/upload",
    "description": "Handling nette/http file uploads. Intended to be used with kdyby/validator and/or arachne/forms.",
    "type": "library",
    "keywords": [ "nette", "arachne", "upload" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jáchym Toušek",
            "homepage": "http://enumag.cz",
            "email": "enumag@gmail.com"
        }
    ],
    "require": {
        "php": "^5.4.0|^7.0.0",
        "nette/http": "^2.3.0"
    },
    "require-dev": {
        "php": "^5.5.0|^7.0.0",
		"arachne/bootstrap": "^0.2.1",
        "arachne/forms": "dev-separate",
		"codeception/codeception": "^2.2.0",
		"codeception/mockery-module": "^0.2.2",
		"kdyby/validator": "^0.4.0"
    },
    "autoload": {
        "psr-4": {
            "Arachne\\Upload\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Integration\\": "tests/integration/src"
        }
    }
}
